<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ page trimDirectiveWhitespaces="true" %>
<head>
<title></title>

</head>
<body>

<!-- 대분류는 h5 -->
<!-- 폰트사이즈는 기본으로(1rem, 16px) -->

<!-- button -->
<!-- 파랑 -->
<button type="button" class="btn btnBlue btn-sm text-white" style="background-color:#2A5F9E;">확인</button>
<!-- 회색 -->
<button type="button" class="btn btnGray btn-sm text-white" style="background-color:#666666;">확인</button>
<!-- 빨강 -->
<button type="button" class="btn btnRed btn-sm text-white" style="background-color:#B94040;">확인 </button>


<!-- 테이블에 들어가는 작은버튼 -->
<!-- 파랑 -->
<button type="button" class="btn btn-xs btnBlue text-white" style="background-color:#2A5F9E;">선택</button>
<!-- 회색 -->
<button type="button" class="btn btn-xs btnGray text-white" style="background-color:#666666;">선택</button>
<!-- 빨강 -->
<button type="button" class="btn btn-xs btnRed text-white" style="background-color:#B94040;">선택</button>


<br><br>

<div class="">
<!-- label + input 라벨 글씨는 설정되어있는 기본으로... -->  
   <label for="">라벨</label>
   <input type="text" class="form-control" id="" value="inputtext" >
</div>


<br><br>

<!-- 테이블 small -->
<table class="table table-hover table-sm" style="font-size: 15px;">
	<thead>
	<tr style="height: 40px;">
		<th>NO</th>
		<th>th</th>
		<th>th</th>
		<th>th</th>
		<th>th</th>
	</tr>
	</thead>
	<tr>
		<td style="height: 40px;"></td>
		<td>td</td>
		<td>td</td>
		<td>td</td>
		<td>td</td>
	</tr>    
</table>
<!-- 테이블 small end -->   


<br><br>


<!-- 검색바 회색  한페이지에 검색바가 여러개 있을때 사용하세여-->					
<div class="input-group input-group-mb-3" style="width: 250px;">
  <input type="text" name="table_search" class="form-control float-right" placeholder="Search">

  <div class="input-group-append">
    <button type="submit" class="btn btn-default">
      <i class="fas fa-search" aria-hidden="true"></i>
    </button>
  </div>
</div>

<br><br>


<!-- 검색바 파랑  검색바가 한페이지에 하나 존재할때 또는 주요검색  -->					
<div class="input-group input-group-mb-3" style="width: 250px;">
  <input type="text" name="table_search" class="form-control float-right" placeholder="Search">

  <div class="input-group-append">
    <button type="submit" class="btn" style="background-color:#2A5F9E;">
      <i class="fas fa-search text-white" aria-hidden="true"></i>
    </button>
  </div>
</div>



</body>
