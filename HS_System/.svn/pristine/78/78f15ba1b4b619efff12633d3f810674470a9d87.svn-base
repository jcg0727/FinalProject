package kr.ac.hs.dto.tuition;

import java.util.Date;

public class TuitionVO {
	
	private String reg_no;
	private String scholar_amount;
	private String semester;
	private String student_no;
	private String tuition_amount;
	private String payment_amount;
	private Date payment_date;
	private String dept_nm;
	private String name;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getReg_no() {
		return reg_no;
	}
	public void setReg_no(String reg_no) {
		this.reg_no = reg_no;
	}
	public String getScholar_amount() {
		return scholar_amount;
	}
	public void setScholar_amount(String scholar_amount) {
		this.scholar_amount = scholar_amount;
	}
	public String getDept_nm() {
		return dept_nm;
	}
	public void setDept_nm(String dept_nm) {
		this.dept_nm = dept_nm;
	}
	public String getSemester() {
		return semester;
	}
	public void setSemester(String semester) {
		this.semester = semester;
	}
	public String getStudent_no() {
		return student_no;
	}
	public void setStudent_no(String student_no) {
		this.student_no = student_no;
	}
	public String getTuition_amount() {
		return tuition_amount;
	}
	public void setTuition_amount(String tuition_amount) {
		this.tuition_amount = tuition_amount;
	}
	public String getPayment_amount() {
		return payment_amount;
	}
	public void setPayment_amount(String payment_amount) {
		this.payment_amount = payment_amount;
	}
	public Date getPayment_date() {
		return payment_date;
	}
	public void setPayment_date(Date payment_date) {
		this.payment_date = payment_date;
	}
	
}
